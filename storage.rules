rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // --- REGLAS DE DEPURACIÃ“N INSEGURAS ---
    // Permite que cualquier usuario autenticado lea y escriba todos los archivos.
    // Usar solo para pruebas.
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}
